-- Scripts:

local function XJLBR_fake_script() -- Close.LocalScript 
	local script = Instance.new('LocalScript', Close)

	script.Parent.MouseButton1Click:Connect(function()
		 script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(XJLBR_fake_script)()
local function LKKNZ_fake_script() -- Close.LocalScript 
	local script = Instance.new('LocalScript', Close)

	script.Parent.MouseButton1Click:Connect(function()
		 script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(LKKNZ_fake_script)()
local function AHPBDQ_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local frame = script.Parent.Parent.Main
	local open = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if frame.Visible == false then
			frame.Visible = true
		else
			frame.Visible = false
		end
	end)
end
coroutine.wrap(AHPBDQ_fake_script)()
